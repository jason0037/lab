<%- model_class = LabDeviceSupport -%>
<div class="panel">
  <header>
    <span><i class="icon-circle-arrow-up arrow_op"></i></span>
    <span><%=t '.title', :default => model_class.model_name.human.pluralize %></span>
     <span class="new_button">
        <i class="icon-pencil"></i>
        <%= link_to t('.new', :default => t("helpers.links.new")),
                    new_lab_device_support_path%>
     </span>
  </header>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:device_id) %></th>
      <th><%= model_class.human_attribute_name(:rel_id) %></th>
      <th><%= model_class.human_attribute_name(:type) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @lab_device_supports.each do |lab_device_support| %>
      <tr>
        <td><%= link_to lab_device_support.id, lab_device_support_path(lab_device_support) %></td>
        <td><%= lab_device_support.device_id %></td>
        <td><%= lab_device_support.rel_id %></td>
        <td><%= lab_device_support.type %></td>
        <td><%=l lab_device_support.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_lab_device_support_path(lab_device_support), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      lab_device_support_path(lab_device_support),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<div class="pagination">
  <%= will_paginate @lab_device_supports, :container => false %>
</div>
