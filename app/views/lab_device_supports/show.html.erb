<%- model_class = LabDeviceSupport -%>
<div class="metro adjust-lab">
  <nav class="breadcrumbs" style="margin-top: 20px">
    <ul>
      <li><a href="#"><i class="icon-home"></i></a></li>
      <li><a href="#">设备</a></li>
      <li><a href="#"><%= @lab_device_support.device_id %></a></li>
    </ul>
  </nav>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:device_id) %>:</strong></dt>
  <dd><%= @lab_device_support.device_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:rel_id) %>:</strong></dt>
  <dd><%= @lab_device_support.rel_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:type) %>:</strong></dt>
  <dd><%= @lab_device_support.type %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              lab_device_supports_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_lab_device_support_path(@lab_device_support), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              lab_device_support_path(@lab_device_support),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
